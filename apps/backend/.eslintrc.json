{
	"extends": ["prettier", "google"],
	"rules": {
		"indent": "error",
		"unicorn/template-indent": [
			"warn",
			{
				"indent": 8
			},
			{
				"tags": ["outdent", "dedent", "sql", "gql", "styled", "css"],
				"functions": ["dedent", "stripIndent"],
				"selectors": ["templateLiteral"],
				"comments": ["indent"]
			}
		]
	}
}
